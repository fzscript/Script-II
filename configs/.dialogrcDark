#
# Menjalankan file konfigurasi untuk dialog
#
# Kustom Konfigurasi Gelap by "FZ Project"
#
#
# Jenis nilai:
#
# Number     -  <number>
# String     -  "string"
# Boolean    -  <ON|OFF>
# Attribute  -  (foreground,background,highlight?,underline?,reverse?)

# Tetapkan rasio aspek.
aspect = 0

# Atur pemisah (untuk output beberapa widget).
separate_widget = ""

# Tetapkan panjang tab (untuk konversi tab kotak teks).
tab_len = 0

# Buat tab-traversal untuk checklist, dll., Sertakan daftarnya.
visit_items = OFF

# Kotak dialog bayangan? Ini juga menyalakan warna.
use_shadow = OFF

# ON atau OFF dukungan warna
use_colors = ON

# Warna layar
screen_color = (WHITE,BLACK,OFF)

# Warna bayangan
shadow_color = (BLACK,BLACK,OFF)

# Warna kotak dialog
dialog_color = screen_color

# Warna judul kotak dialog
title_color = screen_color

# Warna batas kotak dialog
border_color = (GREEN,BLACK,OFF)

# Warna tombol aktif
button_active_color = (WHITE,GREEN,OFF)

# Warna tombol non-aktif
button_inactive_color = screen_color

# Warna kunci tombol aktif
button_key_active_color = button_active_color

# Warna kunci tombol non-aktif
button_key_inactive_color = screen_color

# Warna label tombol aktif
button_label_active_color = button_active_color

# Warna label tombol non-aktif
button_label_inactive_color = screen_color

# Warna kotak input
inputbox_color = screen_color

# Warna batas kotak input
inputbox_border_color = screen_color

# Warna kotak pencarian
searchbox_color = screen_color

# Warna judul kotak pencarian
searchbox_title_color = title_color

# Warna batas kotak pencarian
searchbox_border_color = border_color

# Warna indikator posisi file
position_indicator_color = title_color

# Warna kotak menu
menubox_color = screen_color

# Warna batas kotak menu
menubox_border_color = border_color

# Warna objek
item_color = (GREEN,BLACK,OFF)

# Warna objek yang dipilih
item_selected_color = (WHITE,BLACK,OFF)

# Warna tag
tag_color = title_color

# Warna tag yang dipilih
tag_selected_color = button_label_active_color

# Warna kunci tag
tag_key_color = button_key_inactive_color

# Warna kunci tag yang dipilih
tag_key_selected_color = button_active_color

# Warna kotak centang
check_color = screen_color

# Warna kotak centang yang dipilih
check_selected_color = button_active_color

# Warna panah atas
uarrow_color = screen_color

# Warna panah bawah
darrow_color = uarrow_color

# Warna objek teks-bantuan
itemhelp_color = screen_color

# Warna teks form aktif
form_active_text_color = (WHITE,BLACK,OFF,ON)

# Warna teks form non-aktif
form_text_color = screen_color

# Warna objek form hanya dapat dibaca
form_item_readonly_color = screen_color

# Warna pengukur kotak dialog
gauge_color = title_color

# Warna batas ke-2 kotak dialog
border2_color = border_color

# Warna batas ke-2 kotak input
inputbox_border2_color = screen_color

# Warna batas ke-2 kotak pencarian
searchbox_border2_color = screen_color

# Warna batas ke-2 kotak dialog
menubox_border2_color = screen_color
